RewriteEngine on

#	handle submodule frontend code
#RewriteCond %{REQUEST_URI} ^.*ui/frontend\.(css|js)/frontend\.(css|js)$ [NC]
#RewriteRule ^(.*)ui/frontend\.(css|js)/frontend\.(css|js)$ lib/backend\.php/demo/ui/frontend\.$2/frontend\.$3 [L]

#	allow css js img
RewriteCond %{REQUEST_URI} !^.*\.(jpg|css|js|gif|png)$ [NC]

# 	forward everything to index.php
RewriteRule ^(.*)$ index.php [L]

